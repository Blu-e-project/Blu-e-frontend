<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".MyPageFragment">

    <ImageView
        android:id="@+id/memberPicture_m"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/rounded_gray"
        android:layout_marginStart="27dp"
        android:layout_marginTop="19dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/memberNickName_m"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="21dp"
        android:text="임금님"
        app:layout_constraintStart_toEndOf="@+id/memberPicture_m"
        app:layout_constraintTop_toTopOf="@+id/memberPicture_m" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="멘티님"
        app:layout_constraintStart_toEndOf="@id/memberNickName_m"
        app:layout_constraintTop_toTopOf="@+id/memberNickName_m" />

    <View
        android:id="@+id/line_m"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_marginTop="96dp"
        android:background="@color/gray"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView_mypage_setting"
        android:layout_width="324dp"
        android:layout_height="544dp"
        android:layout_marginTop="23dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line_m"/>


</androidx.constraintlayout.widget.ConstraintLayout >